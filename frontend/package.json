{
	"name": "frontend",
	"version": "0.1.0",
	"private": true,
	"author": "Josiah Schiess",
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"axios": "^0.21.2",
		"core-js": "^3.6.4",
		"vue": "^2.6.11",
		"vue-router": "^3.1.5",
		"vuetify": "^2.2.11",
		"vuex": "^3.1.2",
		"vuex-persistedstate": "^2.7.1"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^4.2.0",
		"@vue/cli-plugin-eslint": "^4.2.0",
		"@vue/cli-plugin-vuex": "^4.2.2",
		"@vue/cli-service": "^5.0.8",
		"babel-eslint": "^10.0.3",
		"eslint": "^6.7.2",
		"eslint-plugin-vue": "^6.1.2",
		"sass": "^1.19.0",
		"sass-loader": "^8.0.0",
		"vue-cli-plugin-pug": "^1.0.7",
		"vue-cli-plugin-vuetify": "^2.0.5",
		"vue-cli-plugin-vuetify-preset-crane": "^1.0.2",
		"vue-template-compiler": "^2.6.11",
		"vuetify-loader": "^1.3.0"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"rules": {
			"indent": [
				"error",
				"tab"
			],
			"no-console": 0,
			"no-unused-vars": [
				"error"
			]
		},
		"parserOptions": {
			"parser": "babel-eslint"
		},
		"overrides": [
			{
				"files": [
					"**/__tests__/*.{j,t}s?(x)",
					"**/tests/unit/**/*.spec.{j,t}s?(x)",
					"quokka.js"
				],
				"env": {
					"jest": true
				}
			}
		]
	}
}
