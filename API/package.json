{
	"name": "api",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start-staging": "PORT=3000 JWT_TOKEN=secret NODE_ENV=staging node index.js",
		"start-dev": "nodemon index.js",
		"testing": "node -e \"process.env.NODE_ENV='production'; const knex = require('./db')\" --experimental-repl-await -i ",
		"start-production": "node index.js",
		"build": "knex migrate:latest && knex seed:run",
		"build-production": "knex migrate:latest --env production  && knex --env production seed:run"
	},
	"author": "josiah schiess",
	"devDependencies": {
		"nodemon": "^2.0.2",
		"standard": "^14.3.1"
	},
	"dependencies": {
		"bcrypt": "3.0.6",
		"connect-history-api-fallback": "^1.6.0",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-graphql": "^0.9.0",
		"graphql": "^14.6.0",
		"jsonwebtoken": "^8.5.1",
		"knex": "^0.20.10",
		"morgan": "^1.9.1",
		"mysql": "^2.18.1",
		"objection": "^2.1.3",
		"objection-graphql": "^0.4.5",
		"sqlite3": "^4.1.1",
		"uuid-v4": "^0.1.0"
	},
	"nodemonConfig": {
		"restartable": "rs",
		"ignore": [
			"node_modules/**/node_modules"
		],
		"delay": "100",
		"env": {
			"NODE_ENV": "development",
			"PORT": 3000,
			"NODE_CONFIG_DIR": "./config",
			"JWT_TOKEN": "Secret"
		}
	}
}